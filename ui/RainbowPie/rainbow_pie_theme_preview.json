{
  "namespace": "rainbowpie_theme_preview",

  "theme_preview_panel": {
    "type": "panel",
    "controls": [
      {
        "background_preview": {
          "type": "panel",
          "size": [ 230, "100%" ],
          "anchor_from": "left_middle",
          "anchor_to": "left_middle",
          "$background_blur|default": "$rainbowpie_ui_theme_blur_background_texture",
          "$background_normal|default": "$rainbowpie_ui_theme_normal_background_texture",
          "controls": [
            {
              "background@mintui_background.background_fullscreen": {
                "ignored": false,
                "$background_layer": 1
              }
            },
            {
              "blur_layer_top@mintui_background.background_blur": {
                "$background_ingame": true,
                "$background_offset": [ 0, "-100% + 10px" ],
                "$background_children_offset": [ 0, "100% - 10px" ],
                "$background_layer": 2
              }
            },
            {
              "blur_layer_bottom@mintui_background.background_blur": {
                "$background_ingame": true,
                "$background_offset": [ 0, "100% - 6px" ],
                "$background_children_offset": [ 0, "-100% + 6px" ],
                "$background_layer": 2
              }
            },
            {
              "title_image@common_art.title_text": {
                "$title_text_scale": 1.8,
                "offset": [ 2, 8 ],
                "anchor_from": "top_left",
                "anchor_to": "top_left",
                "layer": 3
              }
            },
            {
              "default_fake_button_1": {
                "type": "image",
                "size": [ 60, 9 ],
                "offset": [ 2, -20 ],
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "texture": "textures/ui/White",
                "color": "$rainbowpie_ui_theme_button_background_default_color",
                "alpha": "$rainbowpie_ui_theme_button_background_default_alpha",
                "layer": 4,
                "controls": [
                  {
                    "mini_text_1": {
                      "type": "label",
                      "size": [ "default", 6 ],
                      "offset": [ 0, 0 ],
                      "anchor_from": "center",
                      "anchor_to": "center",
                      "text": "Play",
                      "font_type": "$global_font_type",
                      "backup_font_type": "$global_font_type_backup",
                      "font_scale_factor": 0.6,
                      "layer": 4,
                      "color": "$rainbowpie_ui_theme_button_text_default_color"
                    }
                  }
                ]
              }
            },
