{
  "namespace": "music_player",

  "music_player": {
    "type": "screen",
    "force_render_below": "($is_music_player_disabled or $rainbowpie_ui_flag_newAnimTest)",
    "cache_screen": false,
    "controls": [
      {
        "$music_player_screen@$music_player_screen": {}
      }
    ],
    "$music_player_screen|default": "music_player.music_player_screen",
    "variables": [
      {
        "requires": "$is_music_player_disabled",
        "$music_player_screen": "music_player.music_player_disabled_dialog"
      },
      {
        "requires": "($is_music_player_v2_enabled and not $is_music_player_disabled)",
        "$music_player_screen": "music_player.music_player_v2_screen"
      }
    ]
  },

  "music_player_disabled_dialog@common.base_screen": {
    "button_mappings": [
      {
        "from_button_id": "button.menu_cancel",
        "to_button_id": "button.menu_exit",
        "mapping_type": "global"
      }
    ],
    "$screen_content": "music_player.music_player_disabled_dialog_content",
    "$screen_animations": [
      "@music_player.exit_animation_push",
      "@music_player.exit_animation_pop"
    ]
  },

  "music_player_disabled_dialog_content": {
    "type": "panel",
    "controls": [
      {
        "dialog@common_dialogs.main_panel_one_button": {
          "$text_name": "music_player.title",
          "$child_control": "music_player.label_panel",
          "$button_panel": "music_player.confirm_button_dialog",
          "$show_close_button": false,
          "size": [ 200, 120 ]
        }
      }
    ]
  },

  "confirm_button_dialog@common_buttons.light_text_button": {
    "focus_change_down": "FOCUS_OVERRIDE_STOP",
    "layer": 1,
    "$pressed_button_name": "button.menu_exit",
    "$button_text": "gui.ok"
  },

  "exit_animation_push": {
    "anim_type": "alpha",
    "easing": "out_cubic",
    "duration": 0,
    "from": 1.0,
    "to": 0.0,
    "play_event": "screen.exit_push",
    "end_event": "screen.exit_end"
  },

  "exit_animation_pop": {
    "anim_type": "alpha",
    "easing": "out_cubic",
    "duration": 0,
    "from": 1.0,
    "to": 0.0,
    "play_event": "screen.exit_pop",
    "end_event": "screen.exit_end"
  },

  "label_panel@common.scrolling_panel": {
    "$scrolling_pane_size": [ "100% - 1px", "100%" ],
    "$scrolling_pane_offset": [ 2, 0 ],
    "$scrolling_content": "music_player.disabled_dialog_text_panel",
    "$scroll_size": [ 5, "100% - 4px" ],
    "$show_background": false,
    "layer": 1,
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "$always_handle_scrolling": true
  },

  "disabled_dialog_text_panel": {
    "type": "stack_panel",
    "size": [ "100% - 4px", "100%c" ],
    "anchor_to": "top_middle",
    "anchor_from": "top_middle",
    "offset": [ 0, 0 ],
    "controls": [
      {
        "padding": {
          "type": "panel",
          "size": [ 0, 2 ]
        }
      },
      {
        "disabled_dialog_text": {
          "type": "label",
          "size": [ "100%", "default" ],
          "anchor_to": "top_middle",
          "anchor_from": "top_middle",
          "layer": 2,
          "text": "$disable_music_reason",
          "text_tts": "$disable_music_reason",
          "color": "$body_text_color",
          "font_type": "$global_font_type",
          "backup_font_type": "$global_font_type_backup",
          "shadow": false
        }
      }
    ]
  },

  "music_player_v2_screen@music_player.screen_base": {
